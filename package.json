{
  "name": "tushy-vite-plugin-v2",
  "version": "1.0.7",
  "description": "Vite plugin for our tushy shopify theme",
  "repository": {
    "url": "https://github.com/allanhal/tushy-vite-plugin-v2.git"
  },
  "keywords": [
    "vite",
    "shopify",
    "vite-plugin",
    "tushy"
  ],
  "author": "TUSHY INC",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "npm run watch",
    "build": "tsup src/index.ts --dts --format cjs,esm --clean --publicDir",
    "watch": "tsup src/index.ts --dts --format cjs,esm --watch --publicDir",
    "lint": "eslint .",
    "test": "vitest run",
    "prepare": "install-peers"
  },
  "peerDependencies": {
    "vite": "^4.2.1"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fast-glob": "^3.2.11"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "install-peers-cli": "^2.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.6.3",
    "turbo": "^1.3.0",
    "typescript": "^4.7.4",
    "vitest": "^0.25.7"
  }
}
